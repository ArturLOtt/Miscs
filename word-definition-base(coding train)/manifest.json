{
    "manifest_version": 2,
	"name": "w-d-b",
	"description": "w-d-b",
	"version": "2.0",
	"author": "ArturLOtt",
	"content_scripts": [
		{
			"matches":[
			"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"background": { 
		"script": "background.js"
	},
	"browser_action": {
		"default_icon": "iconX.png",
		"default_popup": "index.html",
		"default_title": "That's a definition!"
	
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Alt+Z"
			}
		}
	}
}